 <%- include("./partials/header")%>


      <div class="container" style="margin-top: 10vh; margin-left: 9vw; max-width: 89vw; padding: 0%;">
          <%if(moviesDump.Response==="False"){%>
            <h1>Movie not found !</h1>
            <%}else{%>
                <h1>Result goes here</h1><br><br>
                <!-- <div class="row"> -->
                <%moviesDump['Search'].forEach(function(movie){%>
                    <div class="card text-white bg-dark mb-3" style="max-width: 540px; display: inline-block; margin-right: 5vw;">
                        <div class="row no-gutters">
                          <div class="col-md-4">
                            <img src="<%=movie.Poster%>" class="card-img" alt="...">
                          </div>
                          <div class="col-md-8">
                            <div class="card-body">
                              <h5 class="card-title"><%=movie.Title%></h5>
                              <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                              <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                            </div>
                          </div>
                        </div>
                      </div>
                    <%})%>
                <!-- </div> -->
            <%}%>
      </div>

      <br><br><br><br>
      

<%- include("./partials/footer")%>